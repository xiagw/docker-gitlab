## domain name
DOMAIN_NAME=example.com

## If the server has an external network address, and enable letsencrypt
# GITLAB_LETSENCRYPT=true
# GITLAB_LETSENCRYPT_RENEW=true
## disable letsencrypt
GITLAB_LETSENCRYPT=false
GITLAB_LETSENCRYPT_RENEW=false
DOMAIN_NAME_GIT=git.example.com
DOMAIN_NAME_GIT_EXT=http://git.example.com
GITLAB_HTTP_PORT=80
GITLAB_HTTPS_PORT=443
GITLAB_SSH_PORT=22
GITLAB_ROOT_PASSWORD=gitlab_secret2
## nginx
NGINX_LISTEN_HTTPS=false
NGINX_REDIRECT_HTTP_TO_HTTPS=false

## nexus
DOMAIN_NAME_NEXUS=nexus.example.com
NEXUS_HTTP_PORT=8081

## acme.sh
DEPLOY_SSH_SERVER=192.168.20.153
DEPLOY_SSH_KEYFILE=/home/ubuntu/ssl/key.key
DEPLOY_SSH_CERTFILE=/home/ubuntu/ssl/cert.crt
DEPLOY_SSH_CAFILE=/home/ubuntu/ssl/ca.crt
DEPLOY_SSH_FULLCHAIN=/home/ubuntu/ssl/fullchain.crt

## nessus
NESSUS_ACTIVE_CODE=your_active_code
NESSUS_USER=root
NESSUS_PASS=password2